<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <dev>
            <sga_mediasync>
                <enabled>0</enabled>
                <patterns id="sga">
                    <p1><![CDATA[#src\s*=\s*["|']?([a-zA-Z0-9_./:-]+/media/[a-zA-Z0-9_./ -]+)["|']?#]]></p1>
                    <p2><![CDATA[#url\s*\(["|']?([a-zA-Z0-9_./:-]+/media/[a-zA-Z0-9_./ -]+)["|']?\)#]]></p2>
                    <p3><![CDATA[#data-original\s*=\s*["|']?([a-zA-Z0-9_./:-]+/media/[a-zA-Z0-9_./ -]+)["|']?#]]></p3>
                    <p4><![CDATA[#data-thumb\s*=\s*["|']?([a-zA-Z0-9_./:-]+/media/[a-zA-Z0-9_./ -]+)["|']?#]]></p4>
                    <p5><![CDATA[#srcset\s*=\s*["|']?([a-zA-Z0-9_./:-]+/media/[a-zA-Z0-9_./ -]+)["|']?#]]></p5>
                </patterns>
                <htaccess_password backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
            </sga_mediasync>
        </dev>
    </default>
</config>
